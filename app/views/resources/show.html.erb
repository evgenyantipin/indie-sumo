<%= render 'shared/page_title', title: @resource.title %>

<%= render 'links' %>

<%= render 'shared/page_description', description: @resource.description %>

<% @resource.lists.each do |list| %>
  <div class="list">
    <h3 class="list__title"><%= list.title %></h3>
    <div class="row">
      <%= render partial: 'resource_summary', collection: list.external_resources %>
    </div>
  </div>
<% end %>